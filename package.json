{
    "name": "rbtv-calendar-update",
    "version": "2.0.0",
    "private": false,
    "scripts": {
        "build": "rm -rf dist && tsc -p . && cp -r node_modules dist/node_modules",
        "clean-packages": "rm -rf dist/node_modules/aws-sdk typescript",
        "zip": "cd dist && zip -r -q $npm_package_name.zip . && cd ..",
        "start": "npm run build && npm run clean-packages && npm run zip && ts-node ./update-lambda.ts $npm_package_name ./dist/$npm_package_name.zip",
        "exec": "npm run build && node dist/run"
    },
    "dependencies": {
        "@aws-sdk/client-lambda": "^3.913.0",
        "@aws-sdk/client-s3": "^3.913.0",
        "googleapis": "^164.0.0",
        "luxon": "^3.7.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "none"
    },
    "devDependencies": {
        "@types/luxon": "^3.7.1",
        "typescript": "^5.9.3"
    }
}
